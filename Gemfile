source 'https://rubygems.org'

group :jekyll_plugins do
  gem 'faraday-retry'
  gem 'wdm', '>= 0.1.0' if Gem.win_platform?
  gem 'tzinfo-data', platforms: [:mingw, :mswin, :x64_mingw]
  gem 'tzinfo'
  gem 'jekyll'
  gem 'jekyll-feed'
  gem 'jekyll-sitemap'
  gem 'jekyll-redirect-from'
  gem 'jemoji'
  gem 'webrick', '~> 1.8'
end

gem 'github-pages'
